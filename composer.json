{
    "name": "salahhusa9/updater",
    "type": "project",
    "description": "Update My laravel Project.",
    "keywords": ["framework", "laravel" , "self-update" , "update-laravel" , "update"],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dev",
        "sort-packages": true
    },
    "extra": {
        "laravel" : {
            "providers" : [
                "Project\\Updater\\UpdaterServiceProvider"

            ],
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Salah eddine bendyab",
            "email": "salah.eddine@bendyab.me"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
